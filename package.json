{
  "name": "pdftrackr",
  "version": "1.0.0",
  "description": "PDFTrackr - Secure PDF Sharing & Analytics SaaS",
  "scripts": {
    "dev": "docker-compose up -d",
    "dev:build": "docker-compose up -d --build",
    "dev:rebuild": "docker-compose down && docker-compose up -d --build",
    "stop": "docker-compose down",
    "restart": "docker-compose restart",
    "logs": "docker-compose logs -f",
    "logs:backend": "docker-compose logs -f backend",
    "logs:frontend": "docker-compose logs -f frontend",
    "db:migrate": "docker-compose exec backend npm run db:migrate",
    "db:seed": "docker-compose exec backend npm run db:seed",
    "clean": "docker-compose down -v --remove-orphans",
    "clean:all": "docker-compose down -v --remove-orphans && docker system prune -f",
    "lint": "biome check .",
    "lint:fix": "biome check --apply .",
    "format": "biome format --write .",
    "format:check": "biome format ."
  },
  "keywords": [
    "pdf",
    "sharing",
    "analytics",
    "saas",
    "nextjs",
    "nodejs"
  ],
  "author": "PDFTrackr Team",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.5.2"
  },
  "dependencies": {
    "@types/next": "^8.0.7",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "lucide-react": "^0.536.0",
    "next": "^15.4.5"
  }
}
