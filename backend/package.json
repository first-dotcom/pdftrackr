{
  "name": "pdftrackr-backend",
  "version": "1.0.0",
  "description": "PDFTrackr backend API server",
  "main": "dist/app/src/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/app/src/index.js",
    "test": "jest",
    "lint": "biome check .",
    "lint:fix": "biome check --apply .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "tsx src/scripts/migrate.ts",
    "db:migrate:prod": "node dist/app/src/scripts/migrate.js",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:seed:prod": "node dist/app/src/scripts/seed.js",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push:pg",
    "depcheck": "depcheck"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.400.0",
    "@aws-sdk/s3-request-presigner": "^3.400.0",
    "@clerk/express": "^1.7.15",
    "@clerk/nextjs": "^4.31.8",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.3",
    "postgres": "^3.4.3",
    "prom-client": "^15.0.0",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.7",
    "drizzle-kit": "^0.19.13",
    "jest": "^29.7.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.1.4",
    "typescript": "^5.2.2"
  }
}
